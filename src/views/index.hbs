{{> header isIndexPage=true}}

<div id="carousel" class="carousel slide banner" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators markers">
    <li data-target="#carousel" data-slide-to="0" class="marker active"></li>
    <li data-target="#carousel" data-slide-to="1" class="marker"></li>
    <li data-target="#carousel" data-slide-to="2" class="marker"></li>
    <li data-target="#carousel" data-slide-to="3" class="marker"></li>
    <li data-target="#carousel" data-slide-to="4" class="marker"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner">
    <div class="item active">
      <img src="/img/slides/slide1.png">
    </div>
    <div class="item">
      <img src="/img/slides/slide2.png">
    </div>
    <div class="item">
      <img src="/img/slides/slide4.png">
    </div>
    <div class="item">
      <img src="/img/slides/slide5.png">
    </div>
    <div class="item">
      <img src="/img/slides/slide3.png">
    </div>
  </div>

  <!-- Controls -->
  <a class="carousel-control arrow arrow-left" href="#carousel" role="button" data-slide="prev"></a>
  <a class="carousel-control arrow arrow-right" href="#carousel" role="button" data-slide="next"></a>
</div>

